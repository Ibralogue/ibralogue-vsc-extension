{
  "name": "ibralogue",
  "displayName": "Ibralogue",
  "icon": "images/Logo.png",
  "publisher": "ibra",
  "description": "Adds support for the greatest Unity dialogue library of all time, Ibralogue.",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ibralogue",
        "aliases": [
          "Ibralogue"
        ],
        "extensions": [
          ".ibra"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ibralogue",
        "scopeName": "source.ibralogue",
        "path": "./syntaxes/ibralogue.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ibralogue/ibralogue-vsc-extension.git"
  }
}
